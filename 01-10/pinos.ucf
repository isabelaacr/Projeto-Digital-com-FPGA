# Clock (exemplo: entrada de 50 MHz no pino P85, confirmar no esquemático da placa)
NET "clk" LOC = "P85";

# Botões (troque pelos pinos que você realmente conectou)
NET "btn_up"    LOC = "P34";
NET "btn_down"  LOC = "P35";
NET "btn_left"  LOC = "P36";
NET "btn_right" LOC = "P37";

# Display 7 segmentos (segmentos a..g)
NET "seg[0]" LOC = "N21";  # a
NET "seg[1]" LOC = "N22";  # b
NET "seg[2]" LOC = "N23";  # c
NET "seg[3]" LOC = "N24";  # d
NET "seg[4]" LOC = "N25";  # e
NET "seg[5]" LOC = "N26";  # f
NET "seg[6]" LOC = "N27";  # g

# Seleção dos displays (AN0..AN3) 
# Se usar só 1 display, ligue apenas AN0
NET "an[0]" LOC = "M15";
NET "an[1]" LOC = "M16";
NET "an[2]" LOC = "L15";
NET "an[3]" LOC = "L16";
